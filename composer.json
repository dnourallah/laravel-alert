{
    "name": "dnourallah/laravel-alert",
    "description": "Laravel alert.",
    "keywords": ["laravel", "alert"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dnourallah/laravel-alert/issues",
        "source": "https://github.com/dnourallah/laravel-alert"
    },
    "authors": [
        {
            "name": "Dhaouadi Nourallah",
            "email": "contact.nourallah@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.6"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "DNourallah\\LaravelAlert\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "DNourallah\\LaravelAlert\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
